<?xml version="1.0" encoding="utf-8" ?>
<TrackSymbols>
	<TrackSymbol Type="Primary">
		<EllipseGeometry Center="0,0" RadiusX="7.5" RadiusY="7.5" />
	</TrackSymbol>
	
	<!-- Special Code 4 Hourglass -->
	<TrackSymbol Type="ModeA" SpecialCode="1200">
		<EllipseGeometry Center="0,0" RadiusX="7.5" RadiusY="7.5" />
	</TrackSymbol>
	<TrackSymbol Type="ModeS" SpecialCode="1200">
		<EllipseGeometry Center="0,0" RadiusX="7.5" RadiusY="7.5" />
	</TrackSymbol>
	
	<!-- SSR Circle -->
	<TrackSymbol Type="ModeA">
		<EllipseGeometry Center="0,0" RadiusX="3.5" RadiusY="3.5" />
	</TrackSymbol>
	<TrackSymbol Type="ModeS">
		<EllipseGeometry Center="0,0" RadiusX="3.5" RadiusY="3.5" />
	</TrackSymbol>
	
	<!-- ADSB Prop -->
	<TrackSymbol Type="ADSB" RotateToTrack="True">
		<PathGeometry>
			<Figure Start="0,-8" FigureBegin="Filled" FigureEnd="Closed">
				<Line>4,4</Line>
				<Line>-4,4</Line>
			</Figure>
		</PathGeometry>
	</TrackSymbol>
	
	<!--Flight plan square-->
	<TrackSymbol Type="FlightPlan" RotateToTrack="True">
		<PathGeometry>
			<Figure Start="0,-8" FigureBegin="Hollow" FigureEnd="Closed">
				<Line>4,4</Line>
				<Line>-4,4</Line>
			</Figure>
		</PathGeometry>
	</TrackSymbol>
	<TrackSymbol Type="Cancelled">
		<EllipseGeometry Center="0,0" RadiusX="3" RadiusY="3" />
	</TrackSymbol>
	<TrackSymbol Type="HistoryMarker" StrokeWidth="1" RotateToTrack="True">
		<PathGeometry>
			<Figure Start="3, 0" FigureBegin="Hollow" FigureEnd="Open">
				<Line>-3,0</Line>
			</Figure>
		</PathGeometry>
	</TrackSymbol>
	<TrackSymbol Type="VelocityMarker" StrokeWidth="0">
		<PathGeometry>
			<Figure Start="0,2" FigureBegin="Hollow" FigureEnd="Open">
				<Line>0,-2</Line>
			</Figure>
		</PathGeometry>
	</TrackSymbol>
	<TrackSymbol Type="SRPTimeMarker" StrokeWidth="0">
		<RectangleGeometry TopLeft="-2,-2" BottomRight="2,2" />
	</TrackSymbol>
	<TrackSymbol Type="SRPDistanceMarker">
		<PathGeometry>
			<Figure Start="-3,-3" FigureBegin="Hollow" FigureEnd="Open">
				<Line>3, 3</Line>
			</Figure>
			<Figure Start="3,-3" FigureBegin="Hollow" FigureEnd="Open">
				<Line>-3, 3</Line>
			</Figure>
		</PathGeometry>
	</TrackSymbol>

	<!-- Radar Coast Crosshair-->	
	<TrackSymbol Type="CoastRadar" StrokeWidth="3">
		<PathGeometry>
			<Figure Start="-3,0" FigureBegin="Hollow" FigureEnd="Open">
				<Line>-7, 0</Line>
			</Figure>
			<Figure Start="0,-3" FigureBegin="Hollow" FigureEnd="Open">
				<Line>0, -7</Line>
			</Figure>
			<Figure Start="0,3" FigureBegin="Hollow" FigureEnd="Open">
				<Line>7, 0</Line>
			</Figure>
			<Figure Start="0,3" FigureBegin="Hollow" FigureEnd="Open">
				<Line>0, 7</Line>
			</Figure>
		</PathGeometry>
	</TrackSymbol>		

	<!-- ADSB Coast Cross-->	
	<TrackSymbol Type="CoastADSB" StrokeWidth="3">
		<PathGeometry>
			<Figure Start="-3,-3" FigureBegin="Hollow" FigureEnd="Open">
				<Line>-7, -7</Line>
			</Figure>
			<Figure Start="3,-3" FigureBegin="Hollow" FigureEnd="Open">
				<Line>7, -7</Line>
			</Figure>
			<Figure Start="3,3" FigureBegin="Hollow" FigureEnd="Open">
				<Line>7, 7</Line>
			</Figure>
			<Figure Start="-3,3" FigureBegin="Hollow" FigureEnd="Open">
				<Line>-7, 7</Line>
			</Figure>
		</PathGeometry>
	</TrackSymbol>		
	
	<!-- Selection Octagon -->
	<TrackSymbol Type="Selection">
		<PathGeometry>
			<Figure Start="11,-5" FigureBegin="Hollow" FigureEnd="Closed">
				<Line>5, -11</Line>
				<Line>-5, -11</Line>
				<Line>-11, -5</Line>
				<Line>-11, 5</Line>
				<Line>-5, 11</Line>
				<Line>5, 11</Line>
				<Line>11, 5</Line>
			</Figure>
		</PathGeometry>
	</TrackSymbol>
			
	<!-- Ground Diamond -->
	<TrackSymbol Type="Ground" StrokeWidth="0">
		<PathGeometry>
			<Figure Start="0,-6" FigureBegin="Filled" FigureEnd="Closed">
				<Line>-4, 2</Line>
				<Line>0, 6</Line>
				<Line>4, 2</Line>
			</Figure>
		</PathGeometry>
	</TrackSymbol>
	
	<!-- Ground Circle -->
	<TrackSymbol Type="GroundLight" StrokeWidth="0">
		<EllipseGeometry Center="0,0" RadiusX="6" RadiusY="6" />
	</TrackSymbol>
	
	<!-- Ground Circle -->
	<TrackSymbol Type="GroundMedium" StrokeWidth="0">
		<EllipseGeometry Center="0,0" RadiusX="6" RadiusY="6" />
	</TrackSymbol>
	
	<!-- Ground Circle -->
	<TrackSymbol Type="GroundHeavy" StrokeWidth="5">
		<EllipseGeometry Center="0,0" RadiusX="5" RadiusY="5" />
	</TrackSymbol>

</TrackSymbols>